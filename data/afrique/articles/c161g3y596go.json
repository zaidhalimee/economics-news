                    "model": {
                      "text": "Technique Feynman : Une méthode facile pour apprendre",
                      "blocks": [
                        {
                          "id": "13236bfd",
                          "type": "fragment",
                          "model": {
                            "text": "Technique Feynman : Une méthode facile pour apprendre",
